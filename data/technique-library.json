    {
      "id": "mbht",
      "name": "Morning Breath Hold Time (MBHT)",
      "short_description": "Your internal fitness tracker - measure oxygen efficiency",
      "category": "assessment",
      "duration_minutes": 2,
      "difficulty": "beginner",
      "nap_day": 1,
      "purpose": "Baseline tracking and daily progress measurement",
      "best_for": ["baseline", "progress_tracking", "self_awareness"],
      "instructions": {
        "summary": "Sit upright, relax with nasal breathing for 1-2 minutes, then hold breath after normal exhale until first sign of discomfort",
        "steps": [
          "Sit upright with straight back first thing in morning",
          "Focus on nasal breathing for 1-2 minutes",
          "After a normal inhale and exhale, start timer",
          "Hold breath until first sign of discomfort (diaphragm twitch)",
          "Record your time in seconds"
        ]
      },
      "scoring": {
        "0-10": "Low oxygenation - potential health concerns",
        "10-20": "Stress/anxiety indicators - room for improvement",
        "20-40": "Average - occasional stress vulnerability",
        "40+": "Optimal - efficient oxygen use",
        "60+": "Yogic level - minimal stress symptoms"
      },
      "source": {
        "course": "Level 1a",
        "lesson": "How to do MBHT",
        "url": "https://members.somabreath.com/courses/products/ee2822d0-21f1-4e46-9d17-85ed64d0792f/categories/583399e1-143b-4a49-9e7c-5e858811d183/posts/2045f71b-96ef-4d63-bee1-fea463478af5",
        "post_id": "2045f71b-96ef-4d63-bee1-fea463478af5"
      },
      "related_source": {
        "lesson": "About MBHT score",
        "url": "https://members.somabreath.com/courses/products/ee2822d0-21f1-4e46-9d17-85ed64d0792f/categories/583399e1-143b-4a49-9e7c-5e858811d183/posts/dd3bf944-0622-431e-9767-b007ca207e8c",
