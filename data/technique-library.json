      }
    },
    {
      "id": "breath_awareness",
      "name": "Breath Awareness Meditation",
      "short_description": "Train towards slower, subtler, more rhythmic breathing",
      "category": "foundation",
      "duration_minutes": 10,
      "difficulty": "beginner",
      "nap_day": 1,
      "purpose": "Foundation practice for all other techniques",
      "best_for": ["anxiety", "stress_relief", "sleep", "physical_tension"],
      "instructions": {
        "summary": "Lie down or sit comfortably, focus on sounds, let thoughts fade, observe breath without controlling",
        "steps": [
          "Lie on back or sit with straight back",
          "Bring awareness to any sound in the environment",
          "Notice the inner voice fading as you become present",
          "Imagine thoughts as a stream with no attachment",
          "Allow breath to naturally slow and become rhythmic",
          "Practice for 10 minutes or until deeply relaxed"
        ]
      },
      "audio_required": true,
      "audio_type": "Trypnaural Brainwave Meditation",
      "source": {
        "course": "Level 1a",
        "lesson": "Breath Awareness Exercise",
        "url": "https://members.somabreath.com/courses/products/ee2822d0-21f1-4e46-9d17-85ed64d0792f/categories/583399e1-143b-4a49-9e7c-5e858811d183/posts/3c7b6ab8-3a03-469b-b45d-c7936b38ca86",
        "post_id": "3c7b6ab8-3a03-469b-b45d-c7936b38ca86"
      }
    },
    {
      "id": "diaphragmatic_breathing",
      "name": "Diaphragmatic Breathing",
      "short_description": "The healthy breathing pattern - nose to belly",
      "category": "foundation",
      "duration_minutes": 5,
      "difficulty": "beginner",
      "nap_day": 2,
      "purpose": "Establish baseline healthy breathing pattern",
      "best_for": ["stress_relief", "anxiety", "physical_tension", "default_state"],
      "instructions": {
        "summary": "Breathe through nose into diaphragm with silent, subtle inhalation and relaxed exhalation",
        "steps": [
          "Place left hand on belly, right hand on chest",
          "Breathe in silently through nose into lower back/belly",
          "Chest should barely move, belly expands",
          "Exhale gently and allow natural 4-second pause",
          "Aim for silent, subtle, smooth breathing"
        ]
      },
      "source": {
        "course": "Level 1a",
        "lesson": "Healthy Breathing Pattern",
        "url": "https://members.somabreath.com/courses/products/ee2822d0-21f1-4e46-9d17-85ed64d0792f/categories/583399e1-143b-4a49-9e7c-5e858811d183/posts/1c87c8b2-e96a-49fc-a08f-e1749ff37469",
