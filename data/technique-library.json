    {
      "id": "coherence_breathing",
      "name": "Coherence Breathing",
      "short_description": "6 breaths/minute for HRV optimization and emotional balance",
      "category": "core",
      "duration_minutes": 10,
      "difficulty": "beginner",
      "nap_day": 3,
      "purpose": "Balance nervous system and regulate emotions",
      "best_for": ["anxiety", "emotional_reactivity", "pre_conversation", "performance_anxiety"],
      "instructions": {
        "summary": "Breathe at 6 breaths per minute with equal inhale/exhale to optimize HRV",
        "steps": [
          "Find quiet, comfortable space",
          "Use headphones for entrainment audio",
          "Sit upright or lie down",
          "Follow 5-second inhale, 5-second exhale pattern",
          "Practice for 10 minutes",
          "Can repeat 2-3x during day when off balance"
        ]
