    {
      "id": "hindu_squat",
      "name": "Hindu Squat",
      "short_description": "Anaerobic exercise to boost MBHT and energy",
      "category": "physical",
      "duration_minutes": 3,
      "difficulty": "beginner",
      "nap_day": 4,
      "purpose": "Increase breath hold time through intermittent hypoxia",
      "best_for": ["low_energy", "physical_tension", "mbht_improvement"],
      "instructions": {
        "summary": "Slow squat with coordinated breathing - 15s exhale down, 5s hold, 15s inhale up",
        "steps": [
          "Stand feet hip-width apart, arms extended forward",
          "Inhale deeply through nose",
          "Exhale through nose for 10-15 seconds while squatting down",
          "Hold squat at bottom for 5 seconds",
          "Inhale for 10-15 seconds while rising",
          "Take recovery breaths, repeat 3-5 times"
        ]
      },
      "benefits": [
        "Induces intermittent hypoxia",
        "Strengthens psoas muscles",
        "Increases breath hold time",
        "Improves oxygen efficiency"
      ],
      "source": {
